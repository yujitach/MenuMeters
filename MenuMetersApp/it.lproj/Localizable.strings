"%ld days %02ld:%02ld:%02ld" = "%1$ld giorni %2$02ld:%3$02ld:%4$02ld";
// Single day format
"%ld day %02ld:%02ld:%02ld" = "%1$ld giorno %2$02ld:%3$02ld:%4$02ld";
